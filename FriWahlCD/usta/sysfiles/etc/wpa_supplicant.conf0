ctrl_interface=/var/run/wpa_supplicant
eapol_version=1
ap_scan=1
fast_reauth=1

network={
    ssid="wkit-802.1x"
    key_mgmt=WPA-EAP
    pairwise=CCMP TKIP
    group=CCMP TKIP
    eap=PEAP
    phase2="auth=MSCHAPV2"
    identity="__rzaccount__"
    password="__rzpassword__"
    ca_cert="/etc/ssl/certs/deutsche-telekom-root-ca-2.pem"
}
